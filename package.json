{
  "name": "slug-academic-organizer",
  "version": "1.0.0",
  "description": "Our project is to create a web app to help students keep track of their educational goals in fulfilling GEs and Major requirements. The current system that UCSC offers is honestly kind of nightmare to use and navigate, so we want to provide platform to make it simple and easy for students to record their progress.",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev2": "concurrently \"npm run server\" \"npm run client\""
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chtzou/Slug-Academic-Organizer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chtzou/Slug-Academic-Organizer/issues"
  },
  "homepage": "https://github.com/chtzou/Slug-Academic-Organizer#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "cheerio": "^1.0.0-rc.2",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "mongoose": "^5.3.4",
    "nodemon": "^1.18.4"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "concurrently": "^3.5.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1"
  }
}
